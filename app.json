{
  "expo": {
    "name": "Oneiros",
    "slug": "oneiros-app",
    "version": "1.0.0",
    "projectId": "b81471aa-9f89-4729-8bf3-5ec9e8ec62e9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/loading_image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dreamjournal.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.dreamjournal.app"
    },
    "scheme": "oneiros-dream-journal",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Oneiros to use Face ID to sign in."
        }
      ],
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow Oneiros to use Face ID to sign in."
        }
      ]
    ],
    "extra": {}
  }
}